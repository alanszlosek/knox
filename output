0;47r1;0a2;0a0aC47;0M"vt.h" 36 lines, 867 characters0;0M#ifndef VT_H
#define VT_H

#include <iterm/core.h>
#include <iterm/unix/ttyio.h>
#include <ncurses.h>

#define VT_RUNNING 1
#define VT_PAUSED 2
#define VT_FOCUS 4

struct virtualTerminal {12;8MWINDOW *window, *wBorder;13;8Mint fd; // should probably set this to -1 when closed14;8M// use active, or something for Running, Pause states15;8Mshort state; // VT_PAUSED VT_RUNNING17;8Mint id; // virtual terminal number18;8Mshort int border, padding;
};

// callbacks for iterm
void vtPrintCharacter(void*, char);
void vtMoveCursor(void*, int, int);
void vtInsertLine(void*);
void vtEraseLine(void*);
void vtScrollRegion(void*, int, int);
void vtScrollUp(void*);
void vtAttributes(void*, short, short, short, short, short, shor29;0Mt, short);
void vtExit(void*);

// Virtual Terminal creation/destroying
struct virtualTerminal *vtCreate(int, int, int, int, int);
void vtDestroy(void*);

#endif
